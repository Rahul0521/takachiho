<p id="notice"><%= notice %></p>

<h1>Listing Tmpr Daily Logs</h1>

<table>
  <thead>
    <tr>
      <th>Raspi</th>
      <th>Time stamp</th>
      <th>Temperature</th>
      <th>Pressure</th>
      <th>Humidity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tmpr_daily_logs.each do |tmpr_daily_log| %>
      <tr>
        <td><%= tmpr_daily_log.raspi_id %></td>
        <td><%= tmpr_daily_log.time_stamp %></td>
        <td><%= tmpr_daily_log.temperature %></td>
        <td><%= tmpr_daily_log.pressure %></td>
        <td><%= tmpr_daily_log.humidity %></td>
        <td><%= link_to 'Show', tmpr_daily_log %></td>
        <td><%= link_to 'Edit', edit_tmpr_daily_log_path(tmpr_daily_log) %></td>
        <td><%= link_to 'Destroy', tmpr_daily_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tmpr daily log', new_tmpr_daily_log_path %>
