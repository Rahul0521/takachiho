<p id="notice"><%= notice %></p>

<h1>Listing Tmpr Logs</h1>

<table>
  <thead>
    <tr>
      <th>Raspi</th>
      <th>Time stamp</th>
      <th>Temperature</th>
      <th>Pressure</th>
      <th>Humidity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tmpr_logs.each do |tmpr_log| %>
      <tr>
        <td><%= tmpr_log.device_id %></td>
        <td><%= tmpr_log.time_stamp %></td>
        <td><%= tmpr_log.temperature %></td>
        <td><%= tmpr_log.pressure %></td>
        <td><%= tmpr_log.humidity %></td>
        <td><%= link_to 'Show', tmpr_log %></td>
        <td><%= link_to 'Edit', edit_tmpr_log_path(tmpr_log) %></td>
        <td><%= link_to 'Destroy', tmpr_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tmpr log', new_tmpr_log_path %>
